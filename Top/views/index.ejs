            </div>
        </section>
        <section class="stats">
            <div class="stat-item_tasks">
                <p class="stat-number"><%= data.tasks %></p> 

                    <svg width="75" height="75" viewBox="0 0 155 154" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="77.5" cy="77" r="77" fill="#D9D9D9" fill-opacity="0.4"/>
                        <path d="M78 94.4444V67.3333V47L96.2812 61.2333L80.0312 73.4333M65.8125 81.8742C53.9019 83.8852 45.5 88.7556 45.5 94.4444C45.5 101.931 60.0507 108 78 108C95.9493 108 110.5 101.931 110.5 94.4444C110.5 88.7556 102.098 83.8852 90.1875 81.8742" stroke="white" stroke-width="8" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <p>Tasks</p> <!-- Текст -->               
            </div>
                <div class="stat-item_friends">
                    <p><%= data.friends %></p>
                    <span>Friends</span>
                </div>
                <div class="stat-item_projects">
                    <p><%= data.doneProjects %></p>
                    <span>Done Projects</span>
                </div>
            </section>

            <section class="projects-overview">
                <div class="projects-column">
                    <h3>Active Projects</h3>
                    <% data.activeProjects.forEach(project => { %>
                    <div class="project">
                        <p><%= project.name %></p>
                        <span><%= project.progress %>%</span>
                    </div>
                    <% }) %>
                </div>

                <div class="colleagues-column">
                    <h3>Colleagues</h3>
                    <% data.colleagues.forEach(colleague => { %>
                    <div class="colleague">
                        <p><%= colleague.name %></p>
                        <span><%= colleague.project %></span>
                    </div>
                    <% }) %>
                </div>

                <div class="requests-column">
                    <h3>Requests</h3>
                    <% data.requests.forEach(request => { %>
                    <div class="request">
                        <p><%= request.name %></p>
                        <span class="<%= request.status %>"><%= request.status %></span>
                    </div>
                    <% }) %>
                </div>
            </section>

            <section class="recent-tasks">
                <h3>Recent Tasks</h3>
                <% data.recentTasks.forEach(task => { %>
                <div class="task">
                    <p><%= task.title %></p>
                    <span><%= task.status %></span>
                </div>
                <% }) %>
            </section>
        </main>
    </div>
</body>
</html>
