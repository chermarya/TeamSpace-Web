            <main>
                
        <base>
   
        <div class="filters">
            <div class="filter-button active" data-filter="Php">New</div>
            <div class="filter-button" data-filter="Ui">Favorite</div>
           
          </div>
        

        <!-- Blog Cards -->
        <div class="blog-cards">
            <% blogData.forEach((post) => { %>
                <div class="card">
                    <img src="/images/<%= post.image %>" alt="<%= post.title %>">
                    <h3><%= post.title %></h3>
                    <p><%= post.description %></p>
                    <div class="info">
                        <img src="/images/<%= post.avatar %>" alt="<%= post.author %>">
                        <div class="text">
                            <div class="header1"><%= post.author %></div>
                            <div class="subhead"><%= post.date %></div>
                        </div>
                        <div class="icon"><svg width="21" height="21" viewBox="0 0 21 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M15.6924 0C16.708 0 17.5674 0.859375 17.5674 1.875V18.75C17.5674 19.7266 16.5127 20.3125 15.6533 19.8438L10.0674 16.5625L4.44238 19.8438C3.58301 20.3125 2.56738 19.7266 2.56738 18.75V1.875C2.56738 0.859375 3.3877 0 4.44238 0H15.6924ZM15.6924 17.6562V2.10938C15.6924 1.99219 15.5752 1.875 15.4189 1.875H4.6377C4.52051 1.875 4.44238 1.99219 4.44238 2.10938V17.6562L10.0674 14.375L15.6924 17.6562Z" fill="#3E3232" fill-opacity="0.5"/>
                            </svg>
                            </div>
                    </div>
                </div>
            <% }) %>
        </div>

          <script src="/js/tabs.js">
             // Tab functionality
        // Tab functionality
        document.querySelectorAll(".filter-button").forEach(button => {
            button.addEventListener("click", () => {
                document.querySelectorAll(".filter-button").forEach(b => b.classList.remove("active"));
                button.classList.add("active");
            });
        });
          </script>
      
            </main>
    </base> </div>
